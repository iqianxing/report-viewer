{
  "name": "report-viewer",
  "description": "Cli for piping a unit test result directly into your browser",
  "version": "0.1.1",
  "homepage": "https://github.com/paulpflug/",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/paulpflug/report-viewer"
  },
  "engines": {
    "node": "*"
  },
  "main": "lib/index.js",
  "bin": {
    "report-viewer": "./index.js"
  },
  "dependencies": {
    "commander": "~2.6.0",
    "socket.io": "~1.3.2",
    "xml2json": "^0.6.1",
    "report-viewer-default": "*",
    "opener":"~1.4.0"
  },
  "devDependencies": {
    "coffee-script": "^1.7.1",
    "nodemon": "^1.3.7",
    "parallelshell": "^1.1.0",
    "rimraf": "^2.3.1"
  },
  "keywords": [
    "socket.io",
    "mocha",
    "report",
    "viewer",
    "xunit",
    "browser",
    "live",
    "debug"
  ],
  "readmeFilename": "README.md",
  "scripts": {
    "clean": "rimraf lib/ && mkdir lib",
    "prebuild": "npm run clean",
    "build": "coffee --compile --output lib/ src/*.coffee",
    "watch": "nodemon -q -w src/ --ext 'coffee' --exec 'npm run build'"
  }
}
